    {
      "name": "patterns",
      "type": "table",
      "path": "patterns.csv",
      "scheme": "file",
      "format": "csv",
      "mediatype": "text/csv",
      "encoding": "utf-8",
      "schema": {
        "fields": [
          {
            "name": "pattern_id",
            "type": "string",
            "title": "Pattern ID",
            "description": "Identifier of the alternation pattern."
          },
          {
            "name": "pattern_alternation",
            "type": "string",
            "title": "Alternation pattern",
